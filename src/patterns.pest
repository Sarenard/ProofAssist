WHITESPACE = _{ " " | "\t" | "\n" }

lambdaexpr = _{
    Goal
}

Goal = { "Goal(" ~ lambdaexpr ~ "," ~ usize ~ ")" }

// Define the grammar for the OP enum
OP = _{
    Load
    | Use
    | Intro
}

Load = { "Load(" ~ string ~ ")" }
Use = { "Use(" ~ string ~ ")" }
Intro = { "Intro" }

string = { "\"" ~ ASCII_ALPHANUMERIC+ ~ "\"" }
usize = @{ ASCII_DIGIT+ }